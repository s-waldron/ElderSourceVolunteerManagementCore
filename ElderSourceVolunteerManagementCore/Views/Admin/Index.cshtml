@model IEnumerable<AppUsers>

<table>
    <tr><th>ID</th><th>User Name</th><th>First Name</th><th>Last Name</th><th>Email</th></tr>
    @if(Model.Count() == 0)
    {
        <tr><td colspan="5" class="text-center">No User Accounts</td></tr>
    }
    else
    {
        foreach(AppUsers user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.UserName</td>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
                <td>@user.Email</td>
                <td><a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-id="@user.Id">Edit</a></td>
            </tr>
        }
    }
</table>
<a class="btn btn-primary" asp-action="Create">Create</a>